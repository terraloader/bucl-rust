{p} strpos "hello world" "world"
echo "strpos: {p}"

# Comparison operators in if
if "9" > "3"
	echo "9 > 3: true"
if "1" < "5"
	echo "1 < 5: true"
if "5" >= "5"
	echo "5 >= 5: true"
if "4" <= "5"
	echo "4 <= 5: true"
if "3" != "4"
	echo "3 != 4: true"

{x} = "hello"
{v} getvar "x"
echo "getvar x: {v}"

# nested variable ref: {parts/{i}} where {i} is computed at runtime
{parts} = "alpha" "beta" "gamma"
{i} = "1"
echo "parts/{i} = {parts/{i}}"
